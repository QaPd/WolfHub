local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "WolfHub", HidePremium = false,IntroText = 'WolfHub' , SaveConfig = true, ConfigFolder = "OrionTest"})

-- Functions

function IY()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Infinite%20Yield.txt"))()
end

function CX()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
end

function AR()
    loadstring(game:HttpGet(('https://pastebin.com/raw/NPwf5R4Q'),true))()
end

function AU()
    loadstring(game:HttpGet('https://pastebin.com/raw/7eCVjmjC'))()
end

function LSGame()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Zu0aX/Life-Sentence/main/Main"))()
end

function CGame()
	loadstring(game:HttpGet("https://pastebin.com/raw/0Xxtfktr"))()
end
-- Tabs

local NewsTab = Window:MakeTab({
	Name = "News",
	Icon = "rbxassetid://12099300467",
	PremiumOnly = false
})

local AdminTab = Window:MakeTab({
	Name = "Admin",
	Icon = "rbxassetid://12099369330",
	PremiumOnly = false
})

local FeTab = Window:MakeTab({
	Name = "FE",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

    local FeHatTab = Window:MakeTab({
        Name = "FE: Hats",
        Icon = "rbxassetid://12099772154",
        PremiumOnly = false
})

local GamesTab = Window:MakeTab({
    Name = "Games",
    Icon = "rbxassetid://12100066268",
    PremiumOnly = false
})

-- Labels

NewsTab:AddLabel("Released!")

-- Buttons

AdminTab:AddButton({
	Name = "Infinite Yield",
	Callback = function()
      		IY()
  	end    
})

FeTab:AddButton({
	Name = "Among Us",
	Callback = function()
      		AU()
  	end    
})

FeHatTab:AddButton({
	Name = "Assault Rifle",
	Callback = function()
      		AR()
  	end    
})

GamesTab:AddButton({
	Name = "Life Sentence",
	Callback = function()
      		LSGame()
  	end    
})

GamesTab:AddButton({
	Name = "Criminality",
	Callback = function()
      		CGame()
  	end    
})

FeHatTab:AddParagraph("Required Hats:","Spec Ops CR-L 4 [Back] , Elemental Crystal Golem - Shoulder Rock")

AdminTab:AddButton({
	Name = "CMD-X",
	Callback = function()
      		CX()
  	end    
})




OrionLib:Init()
